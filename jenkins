pipeline
{
    agent any
    stages
    {
        stage('continuousDownloads_Loans')
        {
            steps
            {
                git 'https://github.com/pranay395/maven112.git'
            }
        }
        stage('continuousBuild_Loans')
        {
            steps
            {
                sh 'mvn package'             
            }
        }
    }
}
